@{
    ViewBag.Title = "Home Page";
}

<div class="row mainImageRow">
    @*<div class="col-xs-3 stretch-row">
            <meter value="35" max="100">Level</meter>
        </div>*@
    <div class="col-xs-12">
        <img src="~/Content/Images/tempLogo.jpg" class="img-responsive mainImageLogo" />
    </div>
</div>

<div class="row">
    <div class="col-xs-12 col-md-4 customGlyphiconRow">
        <div class="row">
            <span class="col-xs-12 glyphicon glyphicon-check customGlyphicon"></span>
            <h2 class="col-xs-12">Some Text</h2>
        </div>
    </div>
    <div class="col-xs-12 col-md-4 customGlyphiconRow">
        <div class="row">
            <span class="col-xs-12 glyphicon glyphicon-question-sign customGlyphicon"></span>
            <h2 class="col-xs-12">Other Exciting Text</h2>
        </div>
    </div>
    <div class="col-xs-12 col-md-4 customGlyphiconRow">
        <div class="row">
            <span class="col-xs-12 glyphicon glyphicon-piggy-bank customGlyphicon"></span>
            <h2 class="col-xs-12">More points about things</h2>
        </div>
    </div>
</div>
<div class="row preGaugeRow">
    <h2 class="col-xs-12">Our Progress</h2>
</div>
<div class="row gaugeRow">    
    <div class="col-xs-12">
        <div class="gaugeContainer img-responsive">
            <div class="gauge-a"></div>
            <div class="gauge-b"></div>
            <div id="rotationGauge" class="gauge-c"></div>
            <div class="gauge-data">
                <h1 id="gaugePercent">72%</h1>
            </div>
        </div>
    </div>
</div>


